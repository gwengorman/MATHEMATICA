Cell[TextData[{
 "Use the functions ",
 ButtonBox["PlotRightEndpointApproximation",
  BaseStyle->"Hyperlink",
  ButtonData->"PlotRightEndpointApproximation"],
 " and ",
 ButtonBox["RightEndpointApproximation",
  BaseStyle->"Hyperlink",
  ButtonData->"RightEndpointApproximation"],
 " to make animation that shows the estimated area enclosed by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "=", "0"}], ",", 
    RowBox[{"y", "=", 
     RowBox[{"3", "+", 
      RowBox[{"2", "cos", " ", "x"}]}]}], ",", 
    RowBox[{"x", "=", 
     RowBox[{"\[Pi]", "/", "2"}]}], ","}], TraditionalForm]],ExpressionUUID->
  "fbe93bad-b621-4445-ade6-92d3ca566e48"],
