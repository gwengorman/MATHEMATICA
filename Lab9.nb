    RowBox[{"func_", ",", "a_", ",", "b_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"f", ",", "h"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"f", "[", "X_", "]"}], "=", 
       RowBox[{"func", "/.", 
        RowBox[{"x", "\[Rule]", "X"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"h", "=", 
       RowBox[{
